#include <stdio.h>
#include <conio.h>
void main()
{
  int sub1, sub2, sub3, sub4, sub5,sub6,sub7,total;
  float per;

  printf("Enter marks of 7 subjects:");
  scanf("%d\n%d\n%d\n%d\n%d\n%d\n%d",&sub1,&sub2, &sub3, &sub4, &sub5,&sub6,&sub7);

  total = sub1+sub2+sub3+sub4+sub5+sub6+sub7;
  per = total/7;

  printf("nTotal Marks: %d",total);
  printf("nTotal Percentage: %f", per);

  if(per>=80)
    printf("nDistinction");
  else if(per>=60)
    printf("nFirst Division");
  else if(per>=50)
    printf("nSeocnd Division");
  else if(per>=31)
    printf("nThird Division");
  else
    printf("nFail");
  getch();
return 0;
}
